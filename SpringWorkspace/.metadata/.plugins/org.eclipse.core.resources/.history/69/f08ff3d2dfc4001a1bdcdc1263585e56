package com.csslect.app.dto;

public class AlarmDTO {
	String alarm_Email, alarm_Title, alarm_Sunday, alarm_Monday, alarm_Tuesday;
	String alarm_Wednesday, alarm_Thursday, alarm_Friday, alarm_Saturday, alarm_Times;
	String alarm_Ringtime1_Hour, alarm_Ringtime1_Minute, alarm_Ringtime2_Hour, alarm_Ringtime2_Minute, alarm_Ringtime3_Hour;
	String alarm_Ringtime3_Minute, alarm_Volume, alarm_Bell, alarm_Vib, alarm_Repeat;
	
	public AlarmDTO() {
	}

	public AlarmDTO(String alarm_Email, String alarm_Title, String alarm_Sunday, String alarm_Monday,
			String alarm_Tuesday, String alarm_Wednesday, String alarm_Thursday, String alarm_Friday,
			String alarm_Saturday, String alarm_Times, String alarm_Ringtime1_Hour, String alarm_Ringtime1_Minute,
			String alarm_Ringtime2_Hour, String alarm_Ringtime2_Minute, String alarm_Ringtime3_Hour,
			String alarm_Ringtime3_Minute, String alarm_Volume, String alarm_Bell, String alarm_Vib,
			String alarm_Repeat) {
		super();
		this.alarm_Email = alarm_Email;
		this.alarm_Title = alarm_Title;
		this.alarm_Sunday = alarm_Sunday;
		this.alarm_Monday = alarm_Monday;
		this.alarm_Tuesday = alarm_Tuesday;
		this.alarm_Wednesday = alarm_Wednesday;
		this.alarm_Thursday = alarm_Thursday;
		this.alarm_Friday = alarm_Friday;
		this.alarm_Saturday = alarm_Saturday;
		this.alarm_Times = alarm_Times;
		this.alarm_Ringtime1_Hour = alarm_Ringtime1_Hour;
		this.alarm_Ringtime1_Minute = alarm_Ringtime1_Minute;
		this.alarm_Ringtime2_Hour = alarm_Ringtime2_Hour;
		this.alarm_Ringtime2_Minute = alarm_Ringtime2_Minute;
		this.alarm_Ringtime3_Hour = alarm_Ringtime3_Hour;
		this.alarm_Ringtime3_Minute = alarm_Ringtime3_Minute;
		this.alarm_Volume = alarm_Volume;
		this.alarm_Bell = alarm_Bell;
		this.alarm_Vib = alarm_Vib;
		this.alarm_Repeat = alarm_Repeat;
	}

	public String getAlarm_Email() {
		return alarm_Email;
	}

	public void setAlarm_Email(String alarm_Email) {
		this.alarm_Email = alarm_Email;
	}

	public String getAlarm_Title() {
		return alarm_Title;
	}

	public void setAlarm_Title(String alarm_Title) {
		this.alarm_Title = alarm_Title;
	}

	public String getAlarm_Sunday() {
		return alarm_Sunday;
	}

	public void setAlarm_Sunday(String alarm_Sunday) {
		this.alarm_Sunday = alarm_Sunday;
	}

	public String getAlarm_Monday() {
		return alarm_Monday;
	}

	public void setAlarm_Monday(String alarm_Monday) {
		this.alarm_Monday = alarm_Monday;
	}

	public String getAlarm_Tuesday() {
		return alarm_Tuesday;
	}

	public void setAlarm_Tuesday(String alarm_Tuesday) {
		this.alarm_Tuesday = alarm_Tuesday;
	}

	public String getAlarm_Wednesday() {
		return alarm_Wednesday;
	}

	public void setAlarm_Wednesday(String alarm_Wednesday) {
		this.alarm_Wednesday = alarm_Wednesday;
	}

	public String getAlarm_Thursday() {
		return alarm_Thursday;
	}

	public void setAlarm_Thursday(String alarm_Thursday) {
		this.alarm_Thursday = alarm_Thursday;
	}

	public String getAlarm_Friday() {
		return alarm_Friday;
	}

	public void setAlarm_Friday(String alarm_Friday) {
		this.alarm_Friday = alarm_Friday;
	}

	public String getAlarm_Saturday() {
		return alarm_Saturday;
	}

	public void setAlarm_Saturday(String alarm_Saturday) {
		this.alarm_Saturday = alarm_Saturday;
	}

	public String getAlarm_Times() {
		return alarm_Times;
	}

	public void setAlarm_Times(String alarm_Times) {
		this.alarm_Times = alarm_Times;
	}

	public String getAlarm_Ringtime1_Hour() {
		return alarm_Ringtime1_Hour;
	}

	public void setAlarm_Ringtime1_Hour(String alarm_Ringtime1_Hour) {
		this.alarm_Ringtime1_Hour = alarm_Ringtime1_Hour;
	}

	public String getAlarm_Ringtime1_Minute() {
		return alarm_Ringtime1_Minute;
	}

	public void setAlarm_Ringtime1_Minute(String alarm_Ringtime1_Minute) {
		this.alarm_Ringtime1_Minute = alarm_Ringtime1_Minute;
	}

	public String getAlarm_Ringtime2_Hour() {
		return alarm_Ringtime2_Hour;
	}

	public void setAlarm_Ringtime2_Hour(String alarm_Ringtime2_Hour) {
		this.alarm_Ringtime2_Hour = alarm_Ringtime2_Hour;
	}

	public String getAlarm_Ringtime2_Minute() {
		return alarm_Ringtime2_Minute;
	}

	public void setAlarm_Ringtime2_Minute(String alarm_Ringtime2_Minute) {
		this.alarm_Ringtime2_Minute = alarm_Ringtime2_Minute;
	}

	public String getAlarm_Ringtime3_Hour() {
		return alarm_Ringtime3_Hour;
	}

	public void setAlarm_Ringtime3_Hour(String alarm_Ringtime3_Hour) {
		this.alarm_Ringtime3_Hour = alarm_Ringtime3_Hour;
	}

	public String getAlarm_Ringtime3_Minute() {
		return alarm_Ringtime3_Minute;
	}

	public void setAlarm_Ringtime3_Minute(String alarm_Ringtime3_Minute) {
		this.alarm_Ringtime3_Minute = alarm_Ringtime3_Minute;
	}

	public String getAlarm_Volume() {
		return alarm_Volume;
	}

	public void setAlarm_Volume(String alarm_Volume) {
		this.alarm_Volume = alarm_Volume;
	}

	public String getAlarm_Bell() {
		return alarm_Bell;
	}

	public void setAlarm_Bell(String alarm_Bell) {
		this.alarm_Bell = alarm_Bell;
	}

	public String getAlarm_Vib() {
		return alarm_Vib;
	}

	public void setAlarm_Vib(String alarm_Vib) {
		this.alarm_Vib = alarm_Vib;
	}

	public String getAlarm_Repeat() {
		return alarm_Repeat;
	}

	public void setAlarm_Repeat(String alarm_Repeat) {
		this.alarm_Repeat = alarm_Repeat;
	}
	
	
}